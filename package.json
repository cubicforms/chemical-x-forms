{
  "name": "@chemical-x/forms",
  "version": "0.3.7",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cubicforms/chemical-x-forms"
  },
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "nuxt-module-build build",
    "release": "pnpm lint && pnpm test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test:watch": "vitest watch"
  },
  "description": "A fully type-safe, schema-driven form library that gives you superpowers. Chemical X included.",
  "author": "Oswald Chisala",
  "bugs": {
    "url": "https://github.com/cubicforms/chemical-x-forms/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./nuxt": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./adapters/*": {
      "types": "./dist/runtime/adapters/*/index.d.ts",
      "import": "./dist/runtime/adapters/*/index.mjs",
      "require": "./dist/runtime/adapters/*/index.cjs"
    }
  },
  "keywords": [
    "vue3",
    "nuxt3",
    "forms",
    "composable",
    "cubic forms",
    "@chemical-x/forms",
    "form manager",
    "chemical x"
  ],
  "license": "MIT",
  "packageManager": "pnpm@9.7.0",
  "type": "module",
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^0.7.6",
    "@nuxt/kit": "^3.16.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxt/test-utils": "^3.17.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@vue/compiler-core": "^3.5.13",
    "@vue/shared": "^3.5.13",
    "changelogen": "^0.5.7",
    "eslint": "^9.22.0",
    "fs-extra": "^11.3.0",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.16.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "5.7.3",
    "unbuild": "^3.5.0",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "@vue/compiler-core": ">=3.0.0",
    "@vue/shared": ">=3.0.0",
    "immer": ">=10.0.0",
    "lodash-es": ">= 4.0.0",
    "nuxt": ">=3.0.0 <4.0.0",
    "vue": ">=3.5.0",
    "zod": ">=3.0.0 <4.0.0"
  }
}
