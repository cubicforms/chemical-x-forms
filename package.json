{
  "name": "@chemical-x/forms",
  "version": "0.3.29",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cubicforms/chemical-x-forms"
  },
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix",
    "format": "prettier --write '**/*.{ts,js,vue,css,scss}'",
    "format:check": "prettier --check '**/*.{ts,js,vue,css,scss}'",
    "check": "npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "typecheck": "tsc --noEmit",
    "prepack": "nuxt-module-build build",
    "release": "pnpm lint && pnpm test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test:watch": "vitest watch",
    "prepare": "husky"
  },
  "description": "A fully type-safe, schema-driven form library that gives you superpowers. Chemical X included.",
  "author": "Oswald Chisala",
  "bugs": {
    "url": "https://github.com/cubicforms/chemical-x-forms/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./nuxt": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./adapters/*": {
      "types": "./dist/runtime/adapters/*/index.d.ts",
      "import": "./dist/runtime/adapters/*/index",
      "require": null
    },
    "./types": {
      "types": "./dist/runtime/types/types-api.d.ts",
      "import": null,
      "require": null,
      "default": "./dist/runtime/types/types-api.d.ts"
    }
  },
  "keywords": [
    "vue3",
    "nuxt3",
    "forms",
    "composable",
    "cubic forms",
    "@chemical-x/forms",
    "form manager",
    "chemical x"
  ],
  "license": "MIT",
  "packageManager": "pnpm@9.7.0",
  "type": "module",
  "lint-staged": {
    "./src/**/*.{ts,vue}": "eslint",
    "./playground/**/*.{ts,vue}": "eslint",
    "./test/**/*.{ts,vue}": "eslint"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^0.7.6",
    "@nuxt/kit": "^3.16.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.1",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vue/compiler-core": "^3.5.13",
    "@vue/shared": "^3.5.13",
    "changelogen": "^0.5.7",
    "cheerio": "^1.0.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "immer": "^10.1.1",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "lodash-es": "^4.17.21",
    "nuxt": "^3.16.1",
    "prettier": "^3.5.3",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.28.0",
    "unbuild": "^3.5.0",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.9",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.1",
    "vue-tsc": "^2.2.8",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "@vue/compiler-core": ">=3.0.0",
    "@vue/shared": ">=3.0.0",
    "immer": ">=10.0.0",
    "lodash-es": ">= 4.0.0",
    "nuxt": ">=3.0.0 <4.0.0",
    "vue": ">=3.5.0",
    "zod": ">=3.0.0 <4.0.0"
  }
}
